{
  "name": "st-api-intencion-venta",
  "version": "1.0.0",
  "description": "unmsm-app",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "mocha test/**/*.ts --reporter mocha-sonarqube-reporter --reporter-options output=xunit.xml --timeout 10000 --exit",
    "test:coverage": "nyc --reporter=lcov --reporter=lcovonly npm run test --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "@types/node": "^13.13.39",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-actuator": "^1.7.0",
    "express-openapi-validator": "3.17.2",
    "http-status": "^1.5.0",
    "pg": "^8.6.0",
    "pino": "^5.17.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sinon": "^9.2.3",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.10",
    "@types/express-actuator": "^1.6.1",
    "@types/mocha": "^5.2.7",
    "@types/pino": "^5.20.0",
    "@types/sinon": "^9.0.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@types/shelljs": "0.8.8",
    "@types/cookie-parser": "1.4.2",
    "@types/multer": "1.4.5",
    "@types/pg": "^7.14.11",
    "@types/cors": "^2.8.10",
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^8.2.1",
    "nodemon": "^1.19.4",
    "pino-pretty": "^3.6.1",
    "prettier": "^1.19.1",
    "shelljs": "^0.8.4",
    "supertest": "^4.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^4.2.4",
    "mocha-sonarqube-reporter": "^1.0.2",
    "nyc": "15.1.0"
  },
  "author": "rhernandezr@ripley.com"
}
